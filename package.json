{
  "name": "vscode-micromamba",
  "displayName": "Micromamba",
  "description": "Make conda environments with micromamba.",
  "version": "0.0.1-alpha.1",
  "publisher": "corker",
  "engines": {
    "vscode": "^1.23.0"
  },
  "author": {
    "name": "Michael Borisov",
    "url": "https://github.com/corker/vscode-micromamba"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/corker/vscode-micromamba"
  },
  "galleryBanner": {
    "theme": "dark",
    "color": "#384357"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "mamba",
    "conda"
  ],
  "main": "./out/extension",
  "icon": "images/vscode-micromamba.png",
  "extensionKind": [
    "workspace"
  ],
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "commands": [
      {
        "command": "corker.micromamba.make.default.env",
        "title": "Mamba: create default environment"
      }
    ]
  },
  "scripts": {
    "ci": "npm run lint && npm run test -- --coverage",
    "clean-out": "rimraf ./out",
    "vscode:prepublish": "npm run clean-out && npm run compile",
    "compile": "webpack --mode production",
    "watch": "webpack --mode development --watch",
    "lint": "eslint \"src/**/*.ts\" \"tests/**/*.ts\" \"*.json\"  \"*.js\" ",
    "test": "jest",
    "watch-test": "npm run test -- --watch"
  },
  "dependencies": {
    "follow-redirects": "^1.13.1",
    "shelljs": "^0.8.4",
    "tar": "6.1.0",
    "vscode-codicons": "^0.0.13",
    "unbzip2-stream": "1.4.3"
  },
  "devDependencies": {
    "@types/follow-redirects": "^1.13.0",
    "@types/istanbul-lib-coverage": "^2.0.3",
    "@types/istanbul-lib-source-maps": "^4.0.1",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.21",
    "@types/shelljs": "^0.8.8",
    "@types/tar": "^4.0.4",
    "@types/vscode": "^1.52.0",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "copy-webpack-plugin": "^7.0.0",
    "coveralls": "^3.1.0",
    "danger": "^10.6.0",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jsdoc": "^31.0.5",
    "eslint-plugin-prefer-arrow": "^1.2.2",
    "eslint-plugin-prettier": "^3.3.1",
    "istanbul-lib-coverage": "^3.0.0",
    "istanbul-lib-source-maps": "^4.0.0",
    "jest": "^26.6.3",
    "jest-snapshot": "^26.6.2",
    "prettier": "^2.2.1",
    "raw-loader": "^4.0.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.14",
    "typescript": "^4.1.3",
    "vscode-test": "^1.4.1",
    "webpack": "^5.15.0",
    "webpack-cli": "^4.3.1"
  },
  "__metadata": {
    "id": "TBD",
    "publisherDisplayName": "corker",
    "publisherId": "TBD"
  }
}
